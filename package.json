{
  "name": "ngx-file-upload",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "prod-start": "ng serve --configuration production -o",
    "build": "ng build",
    "prod-build": "ng build --configuration production --base-href=./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "aws-deploy": "npm run prod-build && aws s3 sync dist/ngx-file-upload/ s3://kumaranis.com --delete",
    "aws-ls": "aws s3 ls s3://kumaranis.com/ --recursive --human-readable --summarize",
    "aws-remove": "aws s3 rb s3://kumaranis.com --force",
    "aws-remove-www": "aws s3 rb s3://www.kumaranis.com --force",
    "aws-cb": "aws s3 mb s3://kumaranis.com --region us-east-1",
    "aws-cb-www": "aws s3 mb s3://www.kumaranis.com --region us-east-1",
    "aws-website": "aws s3 website s3://kumaranis.com/ --index-document index.html --error-document index.html",
    "aws-b-policy": "aws s3api put-bucket-policy --bucket kumaranis.com --policy file://docs/bucketpolicy.json",
    "aws-b-policy-www": "aws s3api put-bucket-policy --bucket www.kumaranis.com --policy file://docs/bucketpolicy-www.json",
    "git-publish": "npm run prod-build && npx angular-cli-ghpages --dir=dist/ngx-file-upload --branch=gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.4.0",
    "@angular/cdk": "~13.3.9",
    "@angular/common": "~13.4.0",
    "@angular/compiler": "~13.4.0",
    "@angular/core": "~13.4.0",
    "@angular/forms": "~13.4.0",
    "@angular/material": "~13.3.9",
    "@angular/platform-browser": "~13.4.0",
    "@angular/platform-browser-dynamic": "~13.4.0",
    "@angular/router": "~13.4.0",
    "rxjs": "~6.6.0",
    "tslib": "~2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "^13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.3.11",
    "@angular/compiler-cli": "~13.4.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "angular-cli-ghpages": "~0.6.2",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.4"
  }
}